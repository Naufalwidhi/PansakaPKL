<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.technobit.pansaka">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_pansaka2"
        android:label="Backofice App"
        android:roundIcon="@drawable/logo_pansaka2"
        android:supportsRtl="true"
        android:theme="@style/Apptheme.NoActionbar"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.FilteredTransaction"></activity>
        <activity
            android:name=".activity.OnBoardingActivity"
            android:theme="@style/Apptheme.NoActionbar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.LoginActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"></activity>
        <activity
            android:name=".activity.MainActivity"
            android:theme="@style/Apptheme.NoActionbar" />
        <activity android:name=".activity.ChangePasswordActivity" />
        <activity
            android:name=".activity.DetailTransaksiActivity"
            android:theme="@style/Apptheme.NoActionbar"></activity>
    </application>

</manifest>